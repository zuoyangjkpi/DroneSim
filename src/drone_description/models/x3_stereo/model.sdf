    <!-- VelocityControl removed - using MulticopterVelocityControl from world file instead -->

    <!-- Odometry publisher plugin -->
    <plugin filename="gz-sim8-odometry-publisher-system" name="gz::sim::systems::OdometryPublisher">
      <odom_frame>world</odom_frame>
      <robot_base_frame>X3/base_link</robot_base_frame>
      <dimensions>3</dimensions>
      <odom_topic>/X3/odometry</odom_topic>
      <odom_publish_frequency>50</odom_publish_frequency>
    </plugin>


  </model>
</sdf>
